    <div class="my-5">
        <h2 class="text-center text-white fw-bold">Contact US</h2>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-10 mx-auto">
                <div class="row">
                    <div class="col-md-6 my-3">
                        <form>
                            <div class="form-floating p-2">
                                <input type="name" name="fname" class="form-control" id="floatingInput1"
                                    placeholder="Enter full name" required />
                                <label for="floatingInput">Full Name</label>
                            </div>
                            <div class="form-floating p-2">
                                <input type="text" name="mobile" class="form-control" id="floatingInput2"
                                    placeholder="Enter mobile no" required />
                                <label for="floatingInput">Mobile No</label>
                            </div>
                            <div class="form-floating p-2">
                                <input type="email" name="email" class="form-control" id="floatingInput3"
                                    placeholder="name@example.com" required />
                                <label for="floatingInput">Email address</label>
                            </div>
                            <div class="p-2 text-center">
                                <button onclick="ayush()"
                                    class="btn btn-outline-info fw-semibold shadow">Submit</button>
                            </div>
                        </form>
                        <ToastContainer />
                    </div>
                    <div class="col-md-6">
                        <img src="contactUs.svg" class="img-fluid animated" alt="contact" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        var form = "https://docs.google.com/forms/u/0/d/e/1FAIpQLScYB0SvJGkn2MYR7hEvl31YW-aLQfjHgDU2B8qrljytR5B5UQ/formResponse"
        function ayush() {
            let formData = new FormData();
            let name = document.getElementById("floatingInput1").value;
            let mobile = document.getElementById("floatingInput2").value
            let email = document.getElementById("floatingInput3").value


            formData.append('entry.1711743947', name);
            formData.append('entry.662759187', mobile);
            formData.append('entry.2098499668', email);

            fetch(form,
                {
                    body: formData,
                    method: "POST"
                }).then((res) => {
                    console.log(res)
                    window.location.href = "/"
                })
        }
    </script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8"
        crossorigin="anonymous"></script>
